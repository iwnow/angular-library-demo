{
  "extends": "./tsconfig.esm2015.json",
  "compilerOptions": {
    "target": "es5",
    "outDir": "./tmp/esm5",
    "rootDir": "./tmp"
  },
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "flatModuleOutFile": "ngx-hello-button.js",
    "flatModuleId": "ngx-hello-button"
  }
}
